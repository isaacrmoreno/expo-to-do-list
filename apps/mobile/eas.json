{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "staging"
      },
      "releaseChannel": "development"
    },
    "beta": {
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "beta"
      },
      "releaseChannel": "beta"
    },
    "production": {
      "env": {
        "ENVIRONMENT": "prod"
      },
      "releaseChannel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
